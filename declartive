pipeline
{
    agent any
    stages
    {
        stage('continuousdownloads')
        {
            steps
            {
                git 'https://github.com/DILLIPKMR/maven.git'
            }
        }
        stage('continuousbuild')
        {
            steps
            {
                sh 'mvn package'
            }
        }      
    }
}
